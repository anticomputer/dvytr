# Example .env file for DevYeeter
# Copy this file to .env and customize for your project
# All variables defined here will be available in the container environment

# Application environment
NODE_ENV=development
DEBUG=true

# API Configuration
API_URL=http://localhost:8080
API_KEY=your-api-key-here

# Database Configuration
DATABASE_URL=postgresql://localhost:5432/mydb
DB_USER=dev
DB_PASSWORD=dev123

# Redis
REDIS_URL=redis://localhost:6379

# Ports
PORT=3000
API_PORT=8080

# Feature Flags
ENABLE_FEATURE_X=true
ENABLE_LOGGING=true

# Custom paths
DATA_DIR=/workspace/data
LOG_DIR=/workspace/logs

# Note: These variables are passed to the container at startup
# Changes to .env require restarting the container: dvytr stop && dvytr run
